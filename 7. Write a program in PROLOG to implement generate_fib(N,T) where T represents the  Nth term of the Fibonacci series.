# Prolog
% Base Case
generate_fib(0, 0).
generate_fib(1, 1).

% Recursive case
generate_fib(N, T) :-
    N > 1,
    N1 is N - 1,
    N2 is N - 2,
    generate_fib(N1, T1),
    generate_fib(N2, T2),
    T is T1 + T2.
# Query
?- generate_fib(0, T).
?- generate_fib(1, T).
?- generate_fib(5, T).
?- generate_fib(10, T).
